
<environments>

  <environment id="dell.laptop" name="Dell Laptop" >

    <setting title="Java JDK" name="JAVA_HOME">
      <option title="None"/>
      <option title="JDK 1.6 amd64" default="true">/opt/jdk-1.6.0_18-amd64</option>
      <option title="JDK 1.6 x86"                 >/opt/jdk-1.6.0_15-i586</option>
      <option title="JDK 1.5 amd64"               >/opt/jdk-1.5.0_22-amd64</option>
      <option title="JDK 1.5 x86"                 >/opt/jdk-1.5.0_21-i586</option>
    </setting>

    <setting title="Maven" name="MAVEN_HOME">
      <option title="None"/>
      <option title="Maven 2.2.x" default="true">/opt/apache-maven-2.2.1</option>
      <option title="Maven 3.0.x"               >/opt/apache-maven-3.0.3</option>
    </setting>

    <setting title="Apache ANT" name="ANT_HOME">
      <option title="None"/>
      <option title="Ant 1.8.0" default="true">/opt/apache-ant-1.8.0</option>
    </setting>

    <setting title="Eclipse" name="ECLIPSE_HOME">
      <option title="None"/>
      <option title="Eclipse Galileo"              >/opt/eclipse-galileo</option>
      <option title="Eclipse Helios" default="true">/opt/eclipse-helios</option>
    </setting>

    <setting title="Tomcat" name="CATALINA_HOME">
      <option title="None"/>
      <option title="Tomcat 5.5"                >/opt/apache-tomcat-5.5</option>
      <option title="Tomcat 6.0"                >/opt/apache-tomcat-6.0.26</option>
      <option title="Tomcat 7.0" default="true" >/opt/apache-tomcat-7.0.6</option>
    </setting>

    <setting title="Ruby" name="RUBY_HOME">
      <option title="None"                     >/opt/ruby-1.9.2-p136</option>
      <option title="Ruby 1.9.2" default="true">/opt/ruby-1.9.2-p136</option>
    </setting>

    <setting title="Node.JS" name="NODEJS_HOME">
      <option title="None"/>
      <option title="Node.JS 0.4.8" default="true">/opt/node-0.4.8</option>
    </setting>

  </environment>

</environments>


