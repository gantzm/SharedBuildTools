
<environments>

  <environment id="d820.laptop" name="Dell D820 Laptop">

    <application title="Java JDK">
      <version title="1.6 x86">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-x86</variable>
      </version>
      <version title="1.6 amd64" default="true">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-amd64</variable>
      </version>
      <version title="1.5 amd64">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-amd64</variable>
      </version>
      <version title="1.5 x86">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-x86</variable>
      </version>
    </application>

    <application title="Maven">
      <version title="2.2.x" default="true">
	<variable name="MAVEN_HOME">/opt/apache-maven-2.2.1</variable>
      </version>
      <version title="3.0.x">
	<variable name="MAVEN_HOME">/opt/apache-maven-3.0.3</variable>
      </version>
    </application>

    <application title="Apache ANT">
      <version title="1.8.0" default="true">
	<variable name="ANT_HOME">/opt/apache-ant-1.8.0</variable>
      </version>

    </application>

    <application title="Eclipse">
      <version title="Galileo">
	<variable name="ECLIPSE_HOME">/opt/eclipse-galileo</variable>
      </version>
      <version title="Helios" default="true">
	<variable name="ECLIPSE_HOME">/opt/eclipse-helios</variable>
      </version>
    </application>

    <application title="Apache Tomcat">
      <version title="5.5.x">
	<variable name="CATALINA_HOME">/opt/apache-tomcat-5.5</variable>
      </version>
      <version title="6.0.x">
	<variable name="CATALINA_HOME">/opt/apache-tomcat-6.0.26a</variable>
      </version>
      <version title="7.0.x" default="true">
	<variable name="CATALINA_HOME">/opt/apache-tomcat-7.0.6</variable>
      </version>
    </application>

    <application title="Ruby">
      <version title="1.9.2" default="true">
	<variable name="RUBY_HOME">/opt/ruby-1.9.2-p136</variable>
      </version>
    </application>

    <application title="Node.js">
      <version title="0.4.8" default="true">
	<variable name="NODEJS_HOME">/opt/node-0.4.8</variable>
      </version>
    </application>

  </environment>

  <environment id="m6600.laptop" name="Dell M6600 Laptop">

    <application title="Java JDK">
      <version title="1.6 amd64" default="true">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-amd64</variable>
      </version>
      <version title="1.6 x86">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-x86</variable>
      </version>
      <version title="1.5 amd64">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-amd64</variable>
      </version>
      <version title="1.5 x86">
	<variable name="JAVA_HOME">/opt/jdk-1.6.0_18-x86</variable>
      </version>
    </application>

  </environment>

  <script if="JAVA_HOME">
    <variable name="JAVA_HOME"/>
    <variable name="JRE_HOME">${JAVA_HOME}</variable>
    <variable name="PATH">${JAVA_HOME}/bin:${PATH}</variable>
  </script>
  
  <script if="MAVEN_HOME">
    <variable name="MAVEN_HOME"/>
    <variable name="PATH">${MAVEN_HOME}/bin:${PATH}</variable>
  </script>
  
  <script if="ANT_HOME">
    <variable name="ANT_HOME"/>
    <variable name="PATH">${ANT_HOME}/bin:${PATH}</variable>
  </script>
  
  <script if="ECLIPSE_HOME">
    <variable name="ECLIPSE_HOME"/>
    <variable name="PATH">${ECLIPSE_HOME}/bin:${PATH}</variable>
  </script>

  <script if="CATALINA_HOME">
    <variable name="CATALINA_HOME"/>
    <variable name="PATH">${CATALINA_HOME}/bin:${PATH}</variable>
  </script>

  <script if="RUBY_HOME">
    <variable name="RUBY_HOME"/>
    <variable name="PATH">${RUBY_HOME}/bin:${PATH}</variable>
  </script>

  <script if="NODEJS_HOME">
    <variable name="NODEJS_HOME"/>
    <variable name="PATH">${NODEJS_HOME}/bin:${PATH}</variable>
  </script>

</environments>


